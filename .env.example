# Konfigurasi API Gemini
GEMINI_API_KEY=your_gemini_api_key_here

# Port untuk server (untuk Pterodactyl Panel)
PORT=3000

# Mode development atau production
NODE_ENV=production

# Timeout untuk respons AI (dalam milidetik)
AI_TIMEOUT=30000

# Nama bot (opsional)
BOT_NAME=WhatsApp Gemini Bot

# Admin WhatsApp number (format: 628xxxxx)
ADMIN_NUMBER=

# Enable/disable grup chat responses (true/false)
ALLOW_GROUP_CHAT=false